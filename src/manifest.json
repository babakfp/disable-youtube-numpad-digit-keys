{
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch?v=*"],
            "js": ["src/main.ts"],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "16": "icon/icon-16.png",
        "32": "icon/icon-32.png",
        "48": "icon/icon-48.png",
        "96": "icon/icon-96.png",
        "128": "icon/icon-128.png"
    }
}
